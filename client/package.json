{
  "name": "mern-testing-client",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "cd .. && jest --selectProjects client",
    "test:unit": "cd .. && jest --selectProjects client --testPathPattern=\"client/src/tests/unit\"",
    "test:integration": "cd .. && jest --selectProjects client --testPathPattern=\"client/src/tests/integration\"",
    "test:e2e": "cypress run",
    "coverage": "cd .. && jest --selectProjects client --coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "clsx": "^2.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^5.0.2",
    "babel-jest": "^29.7.0",
    "cypress": "^13.13.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "msw": "^2.2.14",
    "vite": "^7.2.2",
    "whatwg-fetch": "^3.6.20"
  }
}
